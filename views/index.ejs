<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>

  <script src="iframeCommunicator.js"></script>
</head>

<body>

  <input type="text" placeholder="Escribe aquÃ­ un valor">
  <button onclick="sendValue()">Enviar al parent</button>

  <script>

    const iframeCommunicator = new IframeCommunicator();

    const sendValue = () => {
      const value = document.querySelector('input').value;
      iframeCommunicator.sendMessage(window.parent, {inputValue: value})
    }
    
  </script>

</body>

</html>